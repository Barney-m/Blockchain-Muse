n\tdata() {\n\t\treturn {\n\t\t\ttitle: \"\",\n\t\t\tauthor: \"\",\n\t\t\tcategory: 2\n\t\t};\n\t},\n\n\tmethods: {\n\t\tasync addSong() {\n\t\t\ttry {\n\t\t\t\tconst ismn = this.newISMN();\n\t\t\t\tawait this.$contract.addSong(this.title, this.author, this.category, ismn);\n\n\t\t\t\tthis.onAddSong({\n\t\t\t\t\ttitle: this.title,\n\t\t\t\t\tauthor: this.author,\n\t\t\t\t\tcategory: this.category,\n\t\t\t\t\tismn: ismn,\n\t\t\t\t\tavailable: true\n\t\t\t\t});\n\t\t\t} catch (error) {\n\t\t\t\tthis.handle(error);\n\t\t\t}\n\t\t},\n\n\t\tnewISMN() {\n\t\t\treturn Math.floor(1000000000000 + Math.random() * 9000000000000);\n\t\t},\n\n\t\tonAddSong(song) {\n\t\t\tthis.title = \"\";\n\t\t\tthis.author = \"\";\n\t\t\tthis.category = 2;\n\n\t\t\tthis.$emit(\"onAddSong\", { name: \"onAddSong\", data: song });\n\t\t}\n\t}\n};\n</script>\n\n<style scoped>\n.category-btns {\n\tpadding-top: 10px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}